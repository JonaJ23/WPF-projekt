<Window x:Class="Store.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Store"
        mc:Ignorable="d"
        Title="Register" Height="450" Width="800">
    <Grid>
        <Label Content="Register" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="315,24,0,0" FontSize="50"/>
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="230,144,0,0" Name="textBlockFirstname" Text="First Name:" VerticalAlignment="Top" Width="110" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="230,186,0,0" Name="textBlockLastName" Text=" UserName:" VerticalAlignment="Top" Width="110" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="230,217,0,0" Name="textBlockEmailId" Text="Email" VerticalAlignment="Top" Width="110" />
        <TextBox Height="24" HorizontalAlignment="Left" Margin="315,143,0,0" Name="textBoxEmail" VerticalAlignment="Top" Width="222" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="315,186,0,0" Name="passwordBox1" VerticalAlignment="Top" Width="222" />
        <!--For password-->
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="315,217,0,0" Name="passwordBoxConfirm" VerticalAlignment="Top" Width="222" />
        <Button Content="Submit" Height="22" HorizontalAlignment="Left" Margin="330,261,0,0" Name="Submit" VerticalAlignment="Top" Width="70" Click="Submit_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Right" Margin="0,260,318,0" Name="button3" VerticalAlignment="Top" Width="70" Click="button3_Click" />

    </Grid>


</Window>
